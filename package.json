{
  "name": "c4codata",
  "version": "1.0.39",
  "bin": {
    "odata-js-generator": "lib/cli/index.js"
  },
  "devDependencies": {
    "@types/jest": "^20.0.7",
    "@types/node": "^8.0.25",
    "browserify": "^14.4.0",
    "jest": "^20.0.4",
    "jest-cli": "^20.0.0",
    "nsp": "^2.6.3",
    "pre-commit": "^1.2.2",
    "rimraf": "^2.6.1",
    "standard-version": "^4.2.0",
    "ts-jest": "^20.0.10",
    "ts-loader": "^2.3.3",
    "tslint": "^5.6.0",
    "tslint-loader": "^3.5.3",
    "typedoc": "^0.11.1",
    "typescript": "2.4.2",
    "webpack": "^3.5.5",
    "webpack-node-externals": "^1.6.0"
  },
  "typings": "lib/index.d.ts",
  "scripts": {
    "clean": "rimraf lib",
    "browserify": "browserify -e lib/index.js --standalone c4Codata --outfile lib/c4codata.browser.js",
    "build": "npm run clean && tsc && npm run browserify",
    "test": "jest",
    "test:watch": "npm test -- --watch",
    "coverage": "npm test -- --coverage --no-cache",
    "coverage:watch": "npm run coverage -- --watch",
    "release": "standard-version && npm pack",
    "prepare": "npm run build && npm test",
    "gendoc": "typedoc --out docs --hideGenerator --readme README.md --ignoreCompilerErrors src"
  },
  "dependencies": {
    "@types/lodash": "^4.14.102",
    "buffer": "^5.0.8",
    "cli": "^1.0.1",
    "isomorphic-fetch": "^2.2.1",
    "lodash": "^4.17.5",
    "mkdirp": "^0.5.1",
    "url-search-params": "^0.10.0",
    "xml2js": "^0.4.19"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "mapCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.ts"
    ],
    "coveragePathIgnorePatterns": [
      "/tests/",
      "/node_modules/",
      "/src/cli",
      "/src/generator"
    ],
    "testEnvironment": "node",
    "testRegex": "tests/.*\\.spec\\.ts$",
    "moduleFileExtensions": [
      "ts",
      "js",
      "json"
    ]
  },
  "description": "c4c odata javascirpt library",
  "homepage": "https://github.com/Soontao/c4codata",
  "author": {
    "name": "Theo Sun",
    "email": "theo.sun@sap.com",
    "url": "https://github.com/Soontao"
  },
  "main": "lib/index.js",
  "keywords": [
    "C4C",
    "SAP",
    "OData"
  ],
  "repository": "Soontao/c4codata",
  "license": "MIT"
}
